@article{fleck_changing_2010,
	title = {Changing aid regimes? U.S. foreign aid from the Cold War to the War on Terror},
	volume = {91},
	issn = {0304-3878},
	url = {https://www.sciencedirect.com/science/article/pii/S0304387809001011},
	doi = {10.1016/j.jdeveco.2009.09.011},
	shorttitle = {Changing aid regimes?},
	abstract = {This paper explores how U.S. bilateral economic aid has changed over time, focusing on how the recent era–in which the War on Terror has played a prominent role in the Bush administration's aid policy–differs from previous eras. In particular, has the renewed geopolitical role of aid coincided with a reduction of aid to the poorest countries or less weight on need in U.S. aid allocation decisions? We start with an analysis of annual U.S. aid budgets from 1955 to 2006. Controlling for domestic political and economic conditions, we find that the War on Terror's effect on the aid budget is significantly larger than is immediately apparent. To explore how the emphasis on need may have changed over time, we use country-level panel data on aid allocations to 119 countries across the same time period. This shows that U.S. aid flows–for the poorest as well as other developing countries–increased with the War on Terror. However, after rising for 35years, the emphasis placed on need has been falling steadily for core aid recipients during the War on Terror.},
	pages = {185--197},
	number = {2},
	journaltitle = {Journal of Development Economics},
	shortjournal = {Journal of Development Economics},
	author = {Fleck, Robert K. and Kilby, Christopher},
	urldate = {2022-09-14},
	date = {2010-03-01},
	langid = {english},
	keywords = {United States, Foreign aid, Cold War, War on Terror},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\nnray\\Zotero\\storage\\2EPGXTW3\\Fleck and Kilby - 2010 - Changing aid regimes U.S. foreign aid from the Co.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\nnray\\Zotero\\storage\\TDE3TC4P\\S0304387809001011.html:text/html},
}

@article{gang_foreign_1990,
