	keywords = {Foreign aid, Aid effort, Ideology, International political economy},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\nnray\\Zotero\\storage\\WDGRSC9N\\Tingley - 2010 - Donors and domestic politics Political influences.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\nnray\\Zotero\\storage\\R2DV65WG\\S1062976909000957.html:text/html},
}

